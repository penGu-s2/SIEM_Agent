{
  "aes_key": "mydreamteam123456778899",
  "config_sync": {
    "enabled": false,
    "url": "https://cbgs.hanhchinhcong.net/getFluentbitConfig?key=mydreamteam123456778899",
    "interval_minutes": 5
  },
  "odbc": {
    "Connection": "DSN=Test_Log_Service",
    "Query": "SELECT * FROM ServiceLogs",
    "Interval_Sec": "60"
  },
  "windows_event": {
    "Channels": "System,Application,Security",
    "Interval_Sec": "1"
  },
  "syslog": {
    "Listen": "0.0.0.0",
    "Port": "514",
    "Mode": "tcp",
    "Parser": "syslog-rfc5424"
  },
  "FluentBit": {
    "Config": {
      "Service": {
        "Flush": 1,
        "Daemon": "Off",
        "Log_Level": "info",
        "Parsers_File": "parsers.conf"
      },
      "Inputs": [
        {
          "Name": "winlog",
          "Tag": "winlog",
          "Channels": "System,Application",
          "Interval_Sec": 1,
          "DB": ".\\winlog.db"
        },
        {
          "Name": "syslog",
          "Tag": "syslog",
          "Listen": "0.0.0.0",
          "Port": 514,
          "Mode": "tcp",
          "Parser": "syslog-rfc5424"
        }
      ],
      "Filters": [
        {
          "Name": "filter_encrypt",
          "Match": "*"         
        }
      ],
      "Outputs": [
        {
          "Name": "file",
          "Match": "winlog",
          "Path": ".\\logs\\",
          "File": "winlog.log",
          "Format": "plain",
          "Retry_Limit": 3
        },
        {
          "Name": "file",
          "Match": "syslog",
          "Path": ".\\logs\\",
          "File": "syslog.log",
          "Format": "plain",
          "Retry_Limit": 3
        }
      ]
    }
  }
}